.container
  .row
    %h2 Parking History
  %table.table.table-hover.table-condensed
    %thead
      %tr
        %th Vehicle Type
        %th Registration Number
        %th Parking Spot
        %th Parked at
        %th Left at
    %tbody
      - if @parkings.present?
        - @parkings.each do |parking|
          %tr
            %td= parking.vehicle.type
            %td= parking.vehicle.registration_number
            %td= parking.parking_spot.name
            %td= parking.start_time
            %td= parking.end_time
  - if @parkings.present?
    .col-sm-12.reports-Header
      #discount-definition-paginator.mb-0
        = will_paginate @parkings
  - else
    No records found
