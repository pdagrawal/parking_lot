.container
  %h2 Parking Spots
  %table.table.table-hover.table-condensed
    %thead
      %tr
        %th Name
        %th Spot Type
        %th Is Occupied
    %tbody
      - if @parking_spots.present?
        - @parking_spots.each do |spot|
          %tr
            %td= spot.name
            %td= spot.spot_type
            %td= spot.occupied
  - if @parking_spots.present?
    .col-sm-12.reports-Header
      #discount-definition-paginator.mb-0
        = will_paginate @parking_spots, :previous_label => 'Previous', :next_label => 'Next'
  - else
    No records found
